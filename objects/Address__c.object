    <validationRules>
        <fullName>Not_allowed_to_change_Main_Account_Addr</fullName>
        <active>true</active>
        <description>you can not modify the Main Account Address flag</description>
        <errorConditionFormula>and (not (isnew()), ischanged(Main_Account_Address__c))</errorConditionFormula>
        <errorDisplayField>Main_Account_Address__c</errorDisplayField>
        <errorMessage>You can not modify the Main Account Address flag</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Synced_address_fields_are_readonly</fullName>
        <active>true</active>
        <description>The Address fields that are synced by trigger from Account when the flag Main_Account_Address__c is true, are read-only for any direct insert or update (ie. not via a trigger that is able to set the bypassUserValidations flag.</description>
        <errorConditionFormula>AND(
  not(bypassUserValidations__c)
 ,Main_Account_Address__c
 ,OR (
   isNew()
  ,isChanged(Street__c)
  ,isChanged(Postal_Code__c)
  ,isChanged(Place__c)
  ,isChanged(Country__c)
  )
)</errorConditionFormula>
        <errorMessage>You are not allowed to create a Main Address or update the synced address fields of a Main Address directly. Update the Account address instead.</errorMessage>
    </validationRules>
